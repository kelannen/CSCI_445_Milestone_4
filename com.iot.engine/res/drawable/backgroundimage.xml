<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape android:shape="rectangle">
            <corners android:radius="20.0dip" />
            <gradient android:startColor="@color/white" android:endColor="@color/white" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/gray" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <gradient android:startColor="@color/gray" android:endColor="@color/gray" android:angle="270.0" android:centerY="0.75" android:centerColor="@color/gray" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress" android:drawable="@drawable/seekbar_progress_bg" />
</layer-list>